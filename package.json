{
  "name": "visualeq-editor",
  "version": "1.0.0",
  "main": "src/server/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node --experimental-worker src/server/index.js",
    "start:highmem": "node --experimental-worker --max-old-space-size=8192 src/server/index.js --highmem",
    "start:skipconvert": "node src/server/index.js --skip-convert",
    "build": "parcel build src/app/index.html",
    "build:watch": "parcel watch src/app/index.html",
    "dev": "yarn build && yarn start",
    "dev:skipconvert": "yarn build && yarn start:skipconvert",
    "dev:highmem": "yarn build && yarn start:highmem",
    "nodemon": "nodemon -w src --ext js,html --exec yarn dev",
    "nodemon:highmem": "nodemon -w src --ext js,html --exec yarn dev:highmem",
    "nodemon:skipconvert": "nodemon -w src --ext js,html --exec yarn dev:skipconvert",
    "test": "npx nodemon -w test --target node --bundle-node-modules --exec \"parcel build --out-dir test-build test/index.js && node test-build/index.js\""
  },
  "dependencies": {
    "canvas": "^2.5.0",
    "express": "^4.17.1",
    "jimp": "^0.6.4",
    "mysql": "^2.17.1",
    "pako": "^1.0.10",
    "parcel": "^1.12.3",
    "redux": "^4.0.4",
    "three": "^0.106.2",
    "three-gltf-loader": "^1.107.0",
    "vblob": "^1.0.2"
  }
}
